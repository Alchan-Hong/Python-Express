
def getintRange():
    a = int(input("월을 입력하시오(1부터 12사이의 값): "))
    b = int(input("일을 입력하시오(1부터 31사이의 값): "))

    if a >= 1 and a <= 12:
        if b >= 1 and b <= 31:
            print(f"입력된 날짜는 {a}월 {b}일입니다.")
    else:
        getintRange()



print("날짜를 입력하시오(월과 일)")


getintRange()

