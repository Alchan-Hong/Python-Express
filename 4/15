
a = 1
b = 3
i = a/b

for i in range(1, 100, 2):
    if i % 2 == 2:
        continue

    print(i,)


while i < 100:
    i / i+2
    print(i)
    if i % 2 == 2:
        break
    i = i + 2


a = 1
b = 3
sum = 0

while a < 100:
    sum = sum + a / b
    a = a + 2
    b = b + 2
    print(sum)
    a = a + 1
