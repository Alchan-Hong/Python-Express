import random

computer_choice = random.randint(1, 3)
user_choice = int(input("선택하시오(1:가위 2:바위 3:보)" ))

print("컴퓨터의 선택 (1:가위 2:바위 3:보) ", computer_choice)


if computer_choice == user_choice:
    print("무승부 입니다.")
elif user_choice+1 == computer_choice or user_choice-2 == computer_choice:
    print("컴퓨터가 이겼음")
else:
    print("당신이 이겼음")


